<p id="notice"><%= notice %></p>

<h1>Listing Ots</h1>

<table>
  <thead>
    <tr>
      <th>Motor type</th>
      <th>Brand</th>
      <th>Power</th>
      <th>Speed</th>
      <th>Voltage</th>
      <th>Current</th>
      <th>Serial</th>
      <th>Oder activities</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ots.each do |ot| %>
      <tr>
        <td><%= ot.motor_type %></td>
        <td><%= ot.brand %></td>
        <td><%= ot.power %></td>
        <td><%= ot.speed %></td>
        <td><%= ot.voltage %></td>
        <td><%= ot.current %></td>
        <td><%= ot.serial %></td>
        <td><%= ot.oder_activities %></td>
        <td><%= link_to 'Show', ot %></td>
        <td><%= link_to 'Edit', edit_ot_path(ot) %></td>
        <td><%= link_to 'Destroy', ot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ot', new_ot_path %>
