<p id="notice"><%= notice %></p>

<h1>Listing Data Sheets</h1>

<table>
  <thead>
    <tr>
      <th>Start date</th>
      <th>Hz</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_sheets.each do |data_sheet| %>
      <tr>
        <td><%= data_sheet.start_date %></td>
        <td><%= data_sheet.hz %></td>
        <td><%= link_to 'Show', data_sheet %></td>
        <td><%= link_to 'Edit', edit_data_sheet_path(data_sheet) %></td>
        <td><%= link_to 'Destroy', data_sheet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data sheet', new_data_sheet_path %>
